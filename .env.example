# API Configuration
REACT_APP_API_URL=http://localhost:3000

# Authentication
REACT_APP_AUTH_TOKEN_KEY=auth_token
REACT_APP_USER_KEY=auth_user

# CORS Configuration
REACT_APP_CORS_ORIGIN=http://localhost:3000

# Development Configuration
REACT_APP_ENV=development

# Security
REACT_APP_MAX_TOKEN_AGE=24h

# Feature Flags
REACT_APP_ENABLE_LOGGING=true

# Note: Create a .env file with your actual values
# and never commit your .env file to version control
